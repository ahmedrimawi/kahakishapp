<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.TransparentTheme">
    </style>

    <style name="Theme">myTheme</style>

    <style name="Theme.AppCompat.Light.ActionBar">MyActionBar</style>

    <style name="MyActionBar" parent="Theme.AppCompat.Light.ActionBar">
        <item name="android:background">@color/actionBar</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="android:background">@color/actionBar</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>


    <color name="theme_color" />
</resources>